  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.1.0",
    "express": "^4.15.2",
    "jsdom": "^9.8.0",
    "json-schema-faker": "^0.3.6",
    "json-server": "^0.10.0",
    "localtunnel": "^1.8.2",
    "mocha": "^3.3.0",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "numeral": "^2.0.6",
    "style-loader": "^0.17.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "whatwg-fetch": "^2.0.3"
  }
